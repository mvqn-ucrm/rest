{
    "name": "mvqn/rest-ucrm",
    "description": "A simple REST Client library for interfacing with UCRM Endpoints.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "ext-curl": "*",
        "mvqn/common": "1.0.0",
        "mvqn/annotations": "1.0.8",
        "mvqn/collections": "1.0.1",
        "mvqn/rest": "1.0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "7.2.7",
        "vlucas/phpdotenv": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "MVQN\\REST\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MVQN\\REST\\": "tests/"
        }
    }
}